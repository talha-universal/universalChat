{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "universal-chat-lib": {
      "projectType": "library",
      "root": "projects/universal-chat-lib",
      "sourceRoot": "projects/universal-chat-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/universal-chat-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/universal-chat-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/universal-chat-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/universal-chat-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "localapp": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/localapp",
      "sourceRoot": "projects/localapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/localapp",
            "index": "projects/localapp/src/index.html",
            "browser": "projects/localapp/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/localapp/tsconfig.app.json",
            "assets": [
              "projects/localapp/src/favicon.ico",
              "projects/localapp/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/localapp/src/styles.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ],
            "server": "projects/localapp/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/localapp/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "localapp:build:production"
            },
            "development": {
              "buildTarget": "localapp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "localapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/localapp/tsconfig.spec.json",
            "assets": [
              "projects/localapp/src/favicon.ico",
              "projects/localapp/src/assets"
            ],
            "styles": [
              "projects/localapp/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "c2f84c07-245a-4280-821f-7529be65cb98"
  }
}
